<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
		"-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
		"https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
	<suppress files=".+\.java" checks="SpringImportOrder" />
	<suppress files=".+Configuration\.java" checks="HideUtilityClassConstructor" />
	<suppress files=".+Application\.java" checks="HideUtilityClassConstructor" />
	<suppress files=".+Tests\.java" checks="AvoidStarImport" />
	<suppress files=".+Tests\.java" checks="AvoidStaticImport" />
	<suppress files=".+Tests\.java" checks="IllegalImport" />
	<suppress files=".+Tests\.java" checks="RequireThis" />
</suppressions>